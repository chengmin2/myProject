.dep_box .dep {
  width: 100%;
  height: 60px;
  line-height: 60px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #f1f1f1;
}

.dep_box .dep text {
  margin-left: 20px;
}

.dep_box .dep icon {
  font-size: 18px;
  margin-right: 20px;
  color: gray;
  line-height: 24px;
}

