.container {
  padding-top: 20px;
  width: 100%;
}

.head {
  width: 100%;
  height: 95px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-bottom: 1px solid #f1f1f1;
  background-color: white;
}

.photo {
  width: 75px;
  height: 75px;
  border-radius: 50%;
}

.tr {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  height: 50px;
  border-bottom: 1px solid #f1f1f1;
  width: 100%;
  background-color: white;
}
.tr text{
  margin: 0px 20px;
}

.btns {
  padding: 20px 0;
  width: 100%;
  height: 100px;
  background-color: white;
  z-index: 100;
}
.btns button{
  width: 80%;
}
