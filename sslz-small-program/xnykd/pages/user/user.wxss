.frame {
  width: 100%;
  display: flex;
  flex-direction: row;
  padding-left: 10px;
  padding-top: 20px;
  padding-bottom: 10px;
  border-bottom: 1px solid #f1f1f1;
}

.frame input {
  margin-left: 30px;
}

.list {
  width: 100%;
  height: 60px;
  line-height: 60px;
  align-items: center;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-bottom: 1px solid #f1f1f1;
}

.list text {
  margin-left: 20px;
}

.list icon {
  font-size: 18px;
  margin-right: 20px;
  color: gray;
  line-height: 24px;
}

.list span {
  font-size: 18px;
  margin-right: 20px;
  color: #2bd5d5;
  line-height: 24px;
}
